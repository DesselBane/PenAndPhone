{
  "name": "pen-and-phone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "deploy": "cross-env IS_GITHUB_DEPLOY=true vue-cli-service build --dest docs/",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:ci": "yarn test:unit --coverage --ci",
    "test:unit:file": "yarn test:unit --bail --findRelatedTests",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:all:markdownlint": "yarn lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint:eslint": "npx eslint --fix .",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn"
  },
  "dependencies": {
    "@vue/reactivity": "^3.0.0",
    "ant-design-vue": "^2.0.0-beta.10",
    "core-js": "3.6.5",
    "reflect-metadata": "^0.1.13",
    "register-service-worker": "1.7.1",
    "typedjson": "^1.6.0-rc2",
    "vue": "3.0.0",
    "vue-router": "4.0.0-beta.12"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-optional-chaining": "7.11.0",
    "@namics/stylelint-bem": "^6.3.1",
    "@types/jest": "24.0.19",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "@vue/cli-plugin-babel": "4.5.0",
    "@vue/cli-plugin-e2e-cypress": "4.5.0",
    "@vue/cli-plugin-eslint": "4.5.0",
    "@vue/cli-plugin-pwa": "4.5.0",
    "@vue/cli-plugin-router": "4.5.0",
    "@vue/cli-plugin-typescript": "4.5.0",
    "@vue/cli-plugin-unit-jest": "4.5.0",
    "@vue/cli-service": "4.5.0",
    "@vue/compiler-sfc": "3.0.0-rc.13",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "5.0.2",
    "@vue/test-utils": "2.0.0-beta.5",
    "babel-eslint": "10.1.0",
    "cross-env": "7.0.2",
    "eslint": "6.7.2",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "7.0.0-beta.3",
    "jest-extended": "^0.11.5",
    "lint-staged": "9.5.0",
    "node-sass": "4.12.0",
    "prettier": "1.19.1",
    "sass-loader": "8.0.2",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "typescript": "3.9.3",
    "vue-jest": "5.0.0-alpha.4"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
